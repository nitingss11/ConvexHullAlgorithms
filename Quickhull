#include<iostream>
using namespace std;

class point
{
   private:
        int _x; 
        int _y;  
   
   public:
        point(int x, int y)
        { 
            _x = x;
            _y = y;
          }
        
        int getX()
        { 
            return _x;
         }
        
        int getY()
        {
            return _y;
         }
 };

int main()
 {
 
  return 0;
}
